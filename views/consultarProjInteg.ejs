<html lang="pt">

<%- include header %>

<div class="row height-3">
        <div class="col-md-2"></div>
        <div class="col-md-8">
            <div class="inline center width-100">
                <span id="headerProjInteg">Selecione o Projeto:</span>                            
            </div>
            
            <div class="height-3">
                <% if (select) { %>
                    <% for(var i=0; i < select.length; i++) { %>                                    
                        <div class="inline">                    
                            <a class="link projIntegrados spaceRight-2" 
                                target="_blank" 
                                href="/integrarProjetoRelatorio?idProjeto=<%= select[i].idProjeto %>">
                                <span id="nameProjInteg"><%= select[i].nome %></span>
                            </a>                    
                        </div>
                    <% } %>
                <% } %>
            </div>
        
        </div>
        
        <div class="col-md-2"></div>
</div>




<%- include footer %>